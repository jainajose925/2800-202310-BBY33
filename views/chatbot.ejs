<!--// PlaceHolder for main merge issue.-->
<%- include("templates/header") %>
    <link rel="stylesheet" type="text/css" href="/css/nav.css">
    <link rel="stylesheet" type="text/css" href="/css/chatbot.css">
</head>
<body>
    <%- include("templates/nav") %>
    <div class="container chat-container">
        <div id="chat">
            <div id="chat-title">
              <h2>Chat!</h2>
            </div>
            <div id="chat-content">
                <p class="userMessage">I'm feeling angry.<span><br>You 5:01PM</span></p>
                <p class="botMessage">
                    I see you're feeling angry.<br>
                    Let's try getting some exercise!
                    <span><br>Bot 5:01PM</span>
                </p>
                <p class="userMessage">I'll try that!<span><br>You 5:02PM</span></p>
                <p class="botMessage">You got this!<span><br>Bot 5:02PM</span></p>
            </div>
            <div id="chat-form">
              <form id="chat-form-input">
                <input type="text" id="usermsg" placeholder="Start a conversation...">
                <button type="submit" id="send"><svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-send"><line x1="22" y1="2" x2="11" y2="13"></line><polygon points="22 2 15 22 11 13 2 9 22 2"></polygon></svg></button>
              </form>
            </div>
        </div>
    </div>
<%- include("templates/footer") %>